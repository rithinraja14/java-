import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        long N = sc.nextLong();
        long reversed = 0;

        while (N > 0) {
            long digit = N % 10;        // Get last digit
            reversed = reversed * 10 + digit;
            N = N / 10;                // Remove last digit
        }

        System.out.println(reversed);

        sc.close();
    }
}
