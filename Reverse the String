import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        
        if (sc.hasNextLine()) {
            String s = sc.nextLine();
            
            
            String reversed = new StringBuilder(s).reverse().toString();
            
            
            System.out.println(reversed);
        }
        
        
    }
}
