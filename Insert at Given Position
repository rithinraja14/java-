import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str1 = sc.nextLine();
        String str2= sc.nextLine();
        int i =sc.nextInt();
        StringBuilder sb= new StringBuilder(str1);
        sb.insert(i,str2);
      
        {
        
        System.out.println(sb);
        }
    }
}
