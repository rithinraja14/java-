import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str1 = sc.nextLine();
        String str2 = sc.nextLine();
         int i=sc.nextInt();
         int j=sc.nextInt();
          StringBuffer sb= new StringBuffer(str1);
        sb.replace(i,j,str2);
      
        {
        
        System.out.println(sb);
        }
    }
}
